function deletePlantedVeggy() {
  const plantedVeggy = document.querySelector("#planted-veggy-card-id-<%=@planted_veggy.id%>");
  plantedVeggy.innerHTML = "<%= j render 'pages/card_mud', planter: @planter, token: @token %>"
  plantedVeggy.setAttribute('id', '<%= @token %>')
}

deletePlantedVeggy();
